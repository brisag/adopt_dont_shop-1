<%= form_with url: '/applications', method: :post, local: true do |form|%>
  <h3><%= form.label "Start an Application" %></h3>
  <p><%= form.label "Name: " %></p>
  <p><%= form.text_field :name %></p>
  <p><%= form.label "Address: " %></p>
  <p><%= form.text_field :address %></p>
  <p><%= form.label "City: " %></p>
  <p><%= form.text_field :city%></p>
  <p><%= form.label "State: " %></p>
  <p><%= form.text_field :state %></p>
  <p><%= form.label "Zip Code: " %></p>
  <p><%= form.text_field :zip_code %></p>
  <%= form.submit 'Submit' %>
<% end %>
